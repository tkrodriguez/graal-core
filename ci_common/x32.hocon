x32.default : ${commonLinux} {
  capabilities: [linux, x32, no_frequency_scaling, tmpfs10g]
  environment : {
    XMX         : "8g"
    XMS         : "8g"
    JVM_CONFIG  : ${jvm-config.default}
    JVM         : "server"
    MACHINE_NAME: "x32"
  }
}

builds += [
  ${x32.default} ${bench-renaissance-hwloc}         { name: "bench-renaissance-linux-x32" }
]
